option_settings:
  "aws:elasticbeanstalk:application:environment":
    DJANGO_SETTINGS_MODULE: "credsite.settings"
    "PYTHONPATH": "/opt/python/current/app/credsite:$PYTHONPATH"
  "aws:elasticbeanstalk:container:python":
    WSGIPath: credsite/credsite_project/wsgi.py
    NumProcesses: 3
    NumThreads: 20
  "aws:elasticbeanstalk:container:python:staticfiles":
    "/static/": "www/static/"
